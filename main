-- ğŸ› ï¸ [CMDR ADMIN BYPASS]
task.spawn(function()
pcall(function()
local CmdrClient = require(rs:WaitForChild("Packages"):WaitForChild("CmdrClient"))
CmdrClient:SetActivationKeys({Enum.KeyCode.F2, Enum.KeyCode.Quote})
if CmdrClient.IsAdmin then
CmdrClient.IsAdmin = function() return true end
end
if CmdrClient.Dispatcher then
CmdrClient.Dispatcher.PermissionLevel = 10
end
end)
end)

-- ğŸ› ï¸ [REKLAM TETÄ°KLEYÄ°CÄ°]
local function ForceAds()
pcall(function()
local GamepassModule = require(rs.Modules.Client.Data.Gamepass)
if GamepassModule.OnAdvertisementsEnabled then GamepassModule.OnAdvertisementsEnabled:Fire() end
local mainGui = lp.PlayerGui:FindFirstChild("MainGUIHandler", true)
if mainGui then
local adStore = mainGui:FindFirstChild("AdStore", true)
if adStore then
adStore.Visible = true
adStore.Position = UDim2.new(0.5, 0, 0.5, 0)
end
end
end)
end

