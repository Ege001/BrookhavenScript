-- --- [2. AI TOOL MASTER: MONEY GUN & ALL TOOLS] ---
local ToolTab = Window:CreateTab("ğŸ“¦ EÅŸya FabrikasÄ±", 4483362458)
ToolTab:CreateSection("Yapay Zeka Destekli EÅŸya Ãœretici")

local function GetToolForcefully(name)
    pcall(function()
        -- 1. ReplicatedStorage taramasÄ±
        local tool = rs:FindFirstChild(name, true)
        if not tool or not tool:IsA("Tool") then
            -- 2. EÄŸer bulamazsa, oyunun gizli paketlerinden (Packages) Ã§ekmeye Ã§alÄ±ÅŸ
            for _, p in pairs(rs:GetDescendants()) do
                if p.Name:lower() == name:lower() and p:IsA("Tool") then
                    tool = p; break
                end
            end
        end

        if tool then
            local c = tool:Clone()
            c.Parent = lp.Backpack
            Rayfield:Notify({Title = "AI TOOL", Content = name .. " BaÅŸarÄ±yla AlÄ±ndÄ±!", Duration = 3})
        else
            -- 3. Brookhaven'a Ã¶zel MoneyGun tetikleyici (Remote Event denemesi)
            local remote = rs:FindFirstChild("ItemRemote", true) or rs:FindFirstChild("GiveItem", true)
            if remote then remote:FireServer(name) end
            Rayfield:Notify({Title = "BÄ°LGÄ°", Content = name .. " aranÄ±yor/istendi...", Duration = 3})
        end
    end)
end

ToolTab:CreateButton({
   Name = "ğŸ’µ MONEY GUN (VÄ°P) - ZORLA AL",
   Callback = function() GetToolForcefully("MoneyGun") end,
})

ToolTab:CreateButton({
   Name = "ğŸ’ TÃœM GÄ°ZLÄ° EÅYALARI TARAVE AL",
   Callback = function()
       local count = 0
       for _, v in pairs(rs:GetDescendants()) do
           if v:IsA("Tool") and count < 15 then
               v:Clone().Parent = lp.Backpack
               count = count + 1
           end
       end
       Rayfield:Notify({Title = "AI", Content = count .. " eÅŸya envantere zorlandÄ±.", Duration = 4})
   end,
})

ToolTab:CreateInput({
   Name = "EÅŸya Ä°smiyle Ã‡aÄŸÄ±r", PlaceholderText = "Ã–rn: Camera",
   Callback = function(t) GetToolForcefully(t) end,
})

