-- --- [3. AI EÅYA FABRÄ°KASI (MONEY GUN FIX)] ---
local ToolTab = Window:CreateTab("ğŸ“¦ EÅŸyalar", 4483362458)

local function ForceGiveTool(name)
    pcall(function()
        -- ReplicatedStorage'da derin tarama
        local found = rs:FindFirstChild(name, true)
        if found and found:IsA("Tool") then
            found:Clone().Parent = lp.Backpack
            Rayfield:Notify({Title = "AI", Content = name .. " AlÄ±ndÄ±!", Duration = 2})
        else
            -- EÄŸer eÅŸya fiziksel deÄŸilse, Remote'u tetikle (Vip Bypass)
            local remote = rs:FindFirstChild("ItemRemote", true) or rs:FindFirstChild("GiveItem", true)
            if remote then 
                remote:FireServer(name)
                Rayfield:Notify({Title = "AI", Content = name .. " Serverdan Ä°stendi!", Duration = 2})
            end
        end
    end)
end

ToolTab:CreateButton({
   Name = "ğŸ’µ MONEY GUN (Zorla)",
   Callback = function() ForceGiveTool("MoneyGun") end,
})

ToolTab:CreateButton({
   Name = "ğŸ”‘ TÃœM ANAHTARLAR (KeyCard/HouseKey)",
   Callback = function() 
       ForceGiveTool("KeyCard")
       ForceGiveTool("HouseKey")
   end,
})

