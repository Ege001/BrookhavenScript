local PlayerTab = Window:CreateTab("ğŸ‘¤ Oyuncu", 4483362458)
PlayerTab:CreateSlider({Name = "WalkSpeed", Range = {16, 500}, Callback = function(v) lp.Character.Humanoid.WalkSpeed = v end})
PlayerTab:CreateToggle({
   Name = "Noclip (Duvar GeÃ§)",
   Callback = function(v)
       _G.noclip = v
       game:GetService("RunService").Stepped:Connect(function()
           if _G.noclip and lp.Character then
               for _, p in pairs(lp.Character:GetDescendants()) do
                   if p:IsA("BasePart") then p.CanCollide = false end
               end
           end
       end)
   end,
})

